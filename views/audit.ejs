<!DOCTYPE html>
<html>
<head>
    <title>Audit Log</title>

    <!-- Correct CSS path -->
    <link rel="stylesheet" type="text/css" href="/usr/387/public/main.css" />
</head>
<body>

<h1>Audit Log</h1>

<table border="1" cellspacing="0" cellpadding="10">
    <tr>
        <th>ID</th>
        <th>Username</th>
        <th>Status</th>
        <th>Timestamp</th>
    </tr>

    <% logs.forEach(log => { %>
    <tr>
        <td><%= log.id %></td>
        <td><%= log.username %></td>
        <td><%= log.status %></td>
        <td><%= log.timestamp %></td>
    </tr>
    <% }); %>

</table>

<p><a href="/usr/387/">Return to Home</a></p>

</body>
</html>
