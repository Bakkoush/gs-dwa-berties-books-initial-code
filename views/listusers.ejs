<!DOCTYPE html>
<html>
<head>
    <title>List of Users</title>
</head>
<body>
    <h1>Registered Users</h1>

    <table border="1" cellpadding="10">
        <tr>
            <th>Username</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
        </tr>

        <% users.forEach(function(user) { %>
        <tr>
            <td><%= user.username %></td>
            <td><%= user.firstName %></td>
            <td><%= user.lastName %></td>
            <td><%= user.email %></td>
        </tr>
        <% }) %>

    </table>

</body>
</html>
